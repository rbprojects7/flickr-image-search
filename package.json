{
  "name": "flickr-search",
  "version": "1.0.0",
  "description": "react express flickr",
  "main": "index.js",
  "scripts": {
    "start": "better-npm-run start",
    "build": "npm run clean && better-npm-run build",
    "clean": "better-npm-run clean",
    "postinstall": "npm run build"
  },
  "betterScripts": {
    "start": {
      "command": "ts-node ./src/server/index",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production",
        "JWT_SECRET": "ilovescotch"
      }
    },
    "clean": {
      "command": "rimraf public"
    },
    "build": {
      "command": "webpack --progress --hide-modules --config ./config/webpack.js",
      "env": {
        "NODE_ENV": "productio"
      }
    }
  },
  "keywords": [
    "react",
    "redux",
    "webpack",
    "babel",
    "universal",
    "router",
    "JWT",
    "auth0"
  ],
  "author": "Rahul Basu",
  "license": "MIT",
